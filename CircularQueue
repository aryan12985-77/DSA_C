#include<stdio.h>
#include<conio.h>
#define MAXSIZE 10
int cq[MAXSIZE],rear=-1,front=-1;

int main(){
int choice;
do{
    printf("----queue-----");
    printf("\n1.Insert\n2.Delete\n3.Display\n4.Exit");
    printf("----------------");
    printf("Enter your choice:");
    scanf("%d",&choice);
    switch(choice){
    case 1:insert();break;
    case 2:delete1();break;
    case 3:display();break;
    default : printf("Invalid choice");

    }
}while(choice!=4);
}
}

